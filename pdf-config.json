{
  "stylesheet": "body { font-family: 'Liberation Serif', serif; font-size: 12pt; line-height: 1.5; } h1 { color: #003366; } h2 { color: #0056b3; } a { color: #0056b3; }",
  "css": "body { padding: 70px; } @page { size: A4; margin: 0; }",
  "body_class": "markdown-body",
  "marked_options": {
    "smartypants": true
  },
  "pdf_options": {
    "format": "A4",
    "margin": "30mm 20mm",
    "printBackground": true,
    "headerTemplate": "<style>section { margin: 0 auto; font-family: system-ui; font-size: 8pt; }</style><section><span class='date'></span></section>",
    "footerTemplate": "<style>section { margin: 0 auto; font-family: system-ui; font-size: 8pt; }</style><section><div>Page <span class='pageNumber'></span> of <span class='totalPages'></span></div></section>"
  },
  "stylesheet_encoding": "utf-8"
}
